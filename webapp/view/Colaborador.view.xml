<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="g3.G3.controller.Colaborador"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Toolbar class="sapContrastPlus" width="100%" id="toolbar01">
						<content>
							<Button xmlns="sap.m" text="Home" id="button01"/>
							<Button xmlns="sap.m" xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" text="Gestor" id="button02"
								press="action" action:wiring="\{'press':\{'navigation':\{'routeName':'Gestor'\}\}\}"/>
							<Button xmlns="sap.m" xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" text="Colaborador" id="button03"
								press="action" action:wiring="\{'press':\{'navigation':\{'routeName':'Colaborador'\}\}\}"/>
						</content>
					</Toolbar>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>